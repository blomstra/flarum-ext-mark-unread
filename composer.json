{
    "name": "blomstra/mark-unread",
    "description": "Mark a discussion as unread",
    "keywords": [
        "flarum"
    ],
    "type": "flarum-extension",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/blomstra/flarum-ext-mark-unread/issues",
        "source": "https://github.com/blomstra/flarum-ext-mark-unread",
        "forum": "https://discuss.flarum.org/d/29369"
    },
    "homepage": "https://blomstra.net",
    "require": {
        "flarum/core": "^1.2.0"
    },
    "authors": [
        {
            "name": "Team Blomstra",
            "email": "development@blomstra.net",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "Blomstra\\MarkUnread\\": "src/"
        }
    },
    "extra": {
        "flarum-extension": {
            "title": "Mark Unread",
            "category": "feature",
            "icon": {
                "name": "fas fa-envelope",
                "backgroundColor": "#EBF1FD",
                "color": "#3a98d0"
            }
        }
    }
}
